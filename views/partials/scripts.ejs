<script src="https://unpkg.com/sweetalert/dist/sweetalert.min.js"></script>


<script>
    function logout() {
        fetch("/login", {
            method: "DELETE"
        })
            .then(res => res.json())
            .then(result => {
                swal("Good job!", result.success.msg, "warning");
                setTimeout(() => {
                    window.location.replace("/")
                }, 2000)
            })
    }
</script>